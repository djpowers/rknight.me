---
permalink: subscribe/changelog/atom.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet href="/subscribe/styles/rss.xsl" type="text/xsl"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Robb Knight Project Changelog</title>
  <link href="https://rknight.me/changelog.xml" rel="self"/>
  <link href="https://rknight.me/log"/>
  <updated>{{ changelog[0].date | dateForFeed }}</updated>
  <id>rknight.me/log</id>
  <author>
    <name>Robb Knight</name>
  </author>
  {%- for cl in changelog %}
    <entry>
        <title>{{ cl.title }}</title>
        <link href="{{ cl.link }}"/>
        <updated>{{ cl.date | dateForFeed }}</updated>
        <id>https://{{ cl.link }}/#{{ cl.date }}</id>
        <content type="html">
            <![CDATA[
                {{ cl.encoded | safe }}
            ]]>
        </content>
    </entry>
  {%- endfor %}
</feed>
