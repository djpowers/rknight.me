<footer id="footer">
  <div class="footer-left">
    &copy; 2012 - <%= new Date().getFullYear() %> <%= config.author || config.title %>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.footer) { %> 
          <li><a href="<%- url_for(theme.footer[i]) %>"><%= i %></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>
